{
  "name": "geo-forms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 4200",
    "start:qa": "ng serve --configuration qa --open --port 4200",
    "start:uat": "ng serve --configuration uat --open --port 4200",
    "hmr": "ng serve --configuration hmr --open --port 4200",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build",
    "build:qa": "npm version patch -m \"Upgrade to %s\" && ng build --configuration qa",
    "build:uat": "ng build --configuration uat",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "node --max_old_space_size=8192 --harmony node_modules/@angular/cli/bin/ng lint",
    "proxy": "dotnet abp generate-proxy -t ng"
  },
  "private": true,
  "dependencies": {
    "@abp/ng.account": "~5.3.4",
    "@abp/ng.components": "~5.3.4",
    "@abp/ng.core": "~5.3.4",
    "@abp/ng.identity": "~5.3.4",
    "@abp/ng.setting-management": "~5.3.4",
    "@abp/ng.tenant-management": "~5.3.4",
    "@abp/ng.theme.basic": "~5.3.4",
    "@abp/ng.theme.shared": "~5.3.4",
    "@angular/animations": "~13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/localize": "~13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@microsoft/applicationinsights-web": "^2.8.1",
    "@ngxs/store": "^3.7.3",
    "@swimlane/ngx-charts": "^20.1.0",
    "@tinymce/tinymce-angular": "6.0.0",
    "angular2-query-builder": "^0.6.2",
    "date-fns": "^2.28.0",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "ng-keyboard-shortcuts": "10.1.17",
    "ngx-extended-pdf-viewer": "^12.1.0",
    "pdfjs-dist": "^2.16.105",
    "powerbi-client-angular": "^3.0.5",
    "primeflex": "^3.3.0",
    "primeicons": "^5.0.0",
    "primeng": "^13.3.0",
    "prismjs": "^1.27.0",
